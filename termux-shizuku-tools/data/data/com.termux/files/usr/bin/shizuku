#!/data/data/com.termux/files/usr/bin/sh

if [ ! -f "$HOME/rish" ]; then
    echo 'Termux access to Shizuku is not configured. To configure, run "shizuku-setup"'
    exit 1
fi

if [ ! -f "$HOME/rish_shizuku.dex" ]; then
    echo 'Termux access to Shizuku is not configured. To configure, run "shizuku-setup"'
    exit 1
fi
echo "Trying to run Shizuku mode..."
sh $HOME/rish
