#!/data/data/com.termux/files/usr/bin/bash

echo "shizuku – Run interactive shizuku shell mode"
echo "shizuku <command> – Execute command via Shizuku"
echo "shizuku-info – Check package (termux-shizuku-tools) info and Shizuku info"
echo "shizuku-open – Open Shizuku app"
echo "shizuku-stop – Stop Shizuku server"
echo "shizuku-help – Command list"
echo 'Instead of "shizuku" in command names, you can use the abbreviated word "shk"'